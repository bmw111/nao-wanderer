                                                                                                <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        # this may not have been set at the time the box is loaded
        self.securityDistance = None
        self.leftBumper = False
        self.rightBumper = False
        self.leftSonar = -1
        self.rightSonar = -1

    def onLoad(self):
        # imports already done on behaviour init
        self.env = wanderer.make_wanderer_environment(self)

    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onStart(self):
        #~ self.onStopped() #~ activate output of the box
        pass

    def onInput_onStop(self):
        self.onUnload() #~ it is recommended to call onUnload of this box in a onStop method, as the code written in onUnload is used to stop the box as well
        pass

    def onInput_onSonarLeftDetected(self, p):
        self.leftSonar = float(p)
        if self.leftSonar <= self.get_security_distance():
            self.env.log("Left sonar "+str(self.leftSonar)+" within security distance "+str(self.get_security_distance()))
            self.triggerEvent("LeftSonar")

    def onInput_onSonarRightDetected(self, p):
        self.rightSonar = float(p)
        if self.rightSonar <= self.get_security_distance():
            self.env.log("Right sonar "+str(self.rightSonar)+" within security distance "+str(self.get_security_distance()))
            self.triggerEvent("RightSonar")

    def onInput_onBumperLeft(self, p):
        self.leftBumper = p > 0
        if self.leftBumper:
            self.env.log("left bumper pressed")
            self.triggerEvent("LeftBumper")

    def onInput_onBumperRight(self, p):
        self.rightBumper = p > 0
        if self.rightBumper:
            self.env.log("right bumper pressed")
            self.triggerEvent("RightBumper")
        
    def triggerEvent(self, source):
        # imports already done on behaviour init
        sensors =  robotstate.Sensors ({ 'LeftBumper' : self.leftBumper, 
                                        'RightBumper' : self.rightBumper, 
                                        'LeftSonar' : self.leftSonar, 
                                        'RightSonar' : self.rightSonar })
        position = wanderer.get_position(self.env)
        ev = event.ObstacleDetected(source, sensors, position)
        self.env.log("Storing ObstacleDetected event "+repr(ev))  
        wanderer.save_event(self.env, ev)    
        self.onStopped()
    
    def get_security_distance(self):
        if self.securityDistance is None:
            self.securityDistance = float(self.env.memory.getData("WandererSecurityDistance"))
        return self.securityDistance]]>
