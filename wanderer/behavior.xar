        #~ controlPoints.append([[ angles[0], [ 2, -yawEffect, 0.0], [ 2, yawEffect, 0.0]]])        
        
        #~ names.append("HeadPitch")
        #~ #times.append([1.0])
        #~ #times.append([1.1 - speed])
        #~ times.append([(1.0 - speed) * (maxTime - minTime) + minTime])
        #~ controlPoints.append([[ angles[1], [ 2, -pitchEffect, 0.0], [ 2, pitchEffect, 0.0]]])        
        
        #~ try:
            #~ self.nCurrentMoveID = ALMotion.post.angleInterpolationBezier(names, times, controlPoints)
            #~ #BUG: IF WE USE POST WE DONT GET THE EXCEPTION AND CANT DEBUG!!!!!
            #~ #ALMotion.angleInterpolationBezier(names, times, controlPoints)
        #~ except BaseException, err:
            #~ self.log( "%s: Bezier Interpolation Error: err: %s" % ( self.boxName, str( err ) ) );  
        # End Method 3        
    
        # This is needed for Method 2 and Method 3        
