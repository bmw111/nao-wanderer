                                                                            <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        # imports already done on behaviour init
        self.env = wanderer.make_wanderer_environment(self)
        self.resources_path = ALFrameManager.getBehaviorPath(self.behaviorId) + "/resources"
    def onUnload(self):
        pass

    def onInput_onStart(self):
        self.log("Execute plan")
        self.perform_next_action()

    def onInput_onStop(self):
        self.onUnload() 

    def onInput_onActionCompleted(self):
        self.env.log("action completed, next action")
        self.perform_next_action()
        
    def perform_next_action(self):
        self.env.log("perform_next_action")
        self.dump_plan_memory()
        executor = wanderer.get_executor_instance(self.env, self)
        executor.perform_next_action()
        self.env.log("after perform next action")
        self.dump_plan_memory()       
    
    # the executor delegates actually doing an action to this method
    def do_action(self, task):
        self.env.log("do_action: "+task.name())
        try:
            # if the action contains an implementation then use that
            method = getattr(task, "execute")
            method(self.env)
            self.perform_next_action()
        except AttributeError:
                # this is ugly but most of these actions are awkward to 
                # implement in plain python so we delegate the action to
                # choreographe boxes which we signal via outputs
            if isinstance(task, action.WalkSideways):
                self.onWalkSideways(task.distance)
            elif isinstance(task, action.WalkStraight):
                self.onWalk(task.distance)
            elif isinstance(task, action.WalkForwardsIndefinitely):
                self.onContinuousWalk()
            elif isinstance(task, action.Turn):
                self.onTurn(task.direction)
            elif isinstance(task, action.Point):
                self.onPoint([task.x, task.y])
            elif isinstance(task, action.Wave):
                self.onWave()
            elif isinstance(task, action.Say):
                # use the key to lookup the text for the current language
                text = self.env.localized_text("defaults", task.textKey)
                self.onSay(text)
            elif isinstance(task, action.Wait):
                self.onWait(task.delaySeconds)
        
    def all_done(self):
        self.env.log("all done")  
        self.onStopped()
        
    def dump_plan_memory(self):
        self.env.log("Planned actions = "+str(self.env.memory.getData("WandererActionsPlanned")))
        self.env.log("Current action = "+str(self.env.memory.getData("WandererActionsInProgress")))
        self.env.log("Completed = "+str(self.env.memory.getData("WandererActionsCompleted")))]]>
