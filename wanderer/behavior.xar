                                                                                                <![CDATA[import random
import math

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        # attributes needed in shuffle mode
        pass

    def onUnload(self):
         #puts code for box cleanup here
        pass

    def onInput_onStart(self):
        hOffset = self.getParameter("horizontalOffset")    
        vOffset = self.getParameter("verticalOffset")       
        bCtrBias = self.getParameter("centerBias")        
        
        head = ALMotion.getAngles("Head", True)
        hHeadAngle = head[0]
        vHeadAngle = head[1]        
                
        if bCtrBias and hHeadAngle > 0:
            hMax = hOffset * math.cos(hHeadAngle) 
            hMin = (-2 * hOffset) + hMax
        elif bCtrBias and hHeadAngle < 0:
            hMin = -hOffset * math.cos(hHeadAngle) 
            hMax = (2 * hOffset) + hMin
        else:
            hMin = -hOffset
            hMax = hOffset            
    
        if bCtrBias and vHeadAngle > 0:
            vMax = vOffset * math.cos(vHeadAngle) 
            vMin = (-2 * vOffset) + vMax
        elif bCtrBias and vHeadAngle < 0:
            vMin = -vOffset * math.cos(vHeadAngle) 
            vMax = (2 * vOffset) + vMin
        else:
            vMin = -vOffset
            vMax = vOffset
        
        hDeg = random.uniform(hMin, hMax)
        vDeg = random.uniform(vMin, vMax)        
        hRad = hDeg * math.pi / 180
        vRad = vDeg * math.pi / 180
                
        self.output_RandomValue([hRad, vRad])]]>
